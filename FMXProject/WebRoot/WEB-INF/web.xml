<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
     <servlet-name>login</servlet-name>
     <servlet-class>servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>login</servlet-name>
     <url-pattern>*.in</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>student</servlet-name>
     <servlet-class>servlet.EditStuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>student</servlet-name>
     <url-pattern>*.do</url-pattern>
  </servlet-mapping>

  <servlet>
     <servlet-name>teacher</servlet-name>
     <servlet-class>servlet.EditTeaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>teacher</servlet-name>
     <url-pattern>*.do2</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>peixun</servlet-name>
     <servlet-class>servlet.EditPeixunServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>peixun</servlet-name>
     <url-pattern>*.do3</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>stuapply</servlet-name>
     <servlet-class>servlet.StuApplyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>stuapply</servlet-name>
     <url-pattern>*.apply</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>teaapply</servlet-name>
     <servlet-class>servlet.TeaApplyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>teaapply</servlet-name>
     <url-pattern>*.apply2</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>stureport</servlet-name>
     <servlet-class>servlet.StuReportServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>stureport</servlet-name>
     <url-pattern>*.report</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>teareview</servlet-name>
     <servlet-class>servlet.TeaReviewServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>teareview</servlet-name>
     <url-pattern>*.review</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>admin</servlet-name>
     <servlet-class>servlet.AdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>admin</servlet-name>
     <url-pattern>*.admin</url-pattern>
  </servlet-mapping>
</web-app>
